  "build": {
    "appId": "id.sideka",
    "directories": {
      "buildResources": "src/resources",
      "output": "packages",
      "app": "dist"
    },
    "asar": true,
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "include": "src/scripts/cscript.nsh"
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Office"
    },
    "extraResources": [
      {
        "from": "src/app/lib/node-adodb/lib/",
        "to": "lib/node-adodb/",
        "filter": [
          "adodb.js"
        ]
      }
    ],
    "extraFiles": [
      {
        "from": "src/assets/",
        "to": "assets",
        "filter": []
      },
      {
        "from": "src/resources/dev-app-update.yml",
        "to": "dev-app-update.yml",
        "filter": []
      }
    ]
  }
}
